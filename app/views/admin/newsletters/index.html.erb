<h1>Subscribers</h1>
 <table class="table">
    <thead>
      <tr>
        <th>Date</th>
        <th>Email</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @newsletters.each do |newsletter| %>
        <tr>
          <td><%= newsletter.created_at%></td>
          <td><%= newsletter.email%></td>
	       </tr>
      <% end %>
    </tbody>
  </table>

<br>
